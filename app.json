{
  "expo": {
    "name": "WoWAI Studio",
    "slug": "vocalify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/vocalify.png",
    "scheme": "vocalify",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/vocalify.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.codewitharun.vocalify"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/vocalify.png",
        "backgroundColor": "#0F172A"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.codewitharun.vocalify"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/vocalify.png"
    },
    "plugins": ["expo-router", "expo-audio", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ebb2ccc8-a278-40f2-ab39-5d27000534b1"
      }
    }
  }
}
